{
  "name": "@f8n/rainmaker-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite",
    "build": "bunx --bun vite build",
    "preview": "bunx --bun vite preview",
    "format": "bunx @biomejs/biome format --write .",
    "lint": "bunx @biomejs/biome lint .",
    "check": "bunx @biomejs/biome check --apply .",
    "type-check": "bun --bun tsc --noEmit",
    "verify": "bun run type-check && bun run build",
    "test:themes": "vitest run src/__tests__/themes.test.ts",
    "test:themes:e2e": "cypress run --spec 'cypress/e2e/themes.cy.ts'",
    "test:themes:all": "npm run test:themes && npm run test:themes:e2e"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.33.1",
    "@hono/node-server": "1.12.0",
    "@hono/zod-validator": "0.2.2",
    "@octokit/plugin-rest-endpoint-methods": "13.2.4",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-dialog": "1.1.4",
    "@radix-ui/react-icons": "1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "async-sema": "3.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "16.4.5",
    "framer-motion": "11.3.8",
    "hono": "4.5.1",
    "lucide-react": "^0.416.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "9.0.1",
    "rehype-raw": "7.0.0",
    "rehype-sanitize": "6.0.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@testing-library/react": "16.0.0",
    "@types/node": "20.14.12",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "10.4.19",
    "postcss": "8.4.40",
    "tailwindcss": "3.4.7",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  }
}
